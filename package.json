{
  "scripts": {
    "clean": "rm -rf lib_es6 lib docs/webpack/*.js dist docs_ts_compiled",
    "build_es6": "tsc --project tsconfig.lib.json",
    "es6_to_es5": "babel lib_es6 --out-dir lib",
    "build_lib": "npm run build_es6 && npm run es6_to_es5 && npm run generate_dts",
    "prepublish": "npm run clean && npm run build",
    "generate_dts": "node dts-generator.js",
    "compile_doc_webpack": "tsc docs_src/webpack/build.ts",
    "build": "npm run build_lib && npm run compile_doc_webpack && webpack --config docs_src/webpack/build.js",
    "run_doc": "npm run compile_doc_webpack && webpack-dev-server --config docs/webpack/build.js",
    "storybook": "start-storybook -p 7001",
    "build-storybook": "build-storybook",
    "test": "npm run unittest",
    "unittest": "jest --config src/__unittests__/jest_unittests.json"
  },
  "name": "react-analytics-context",
  "types": "react-analytics-context.d.ts",
  "version": "0.0.1",
  "description": "configurable react analytics components/helpers/etc.",
  "main": "index.js",
  "author": "devdoomari.ubuntu.tablet <devdoomari@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash": "^4.14.55",
    "@types/node": "^7.0.8",
    "@types/react": "^15.0.16",
    "@types/react-bootstrap": "^0.0.46",
    "@types/react-dom": "^0.14.23",
    "@types/react-router": "^3.0.8",
    "@types/recompose": "^0.20.4",
    "@types/webpack": "^2.2.11",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-polyfill": "^6.23.0",
    "bootstrap": "^3.3.7",
    "console-polyfill": "^0.2.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.27.3",
    "dts-generator": "^2.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "history": "^4.6.0",
    "jest": "^19.0.2",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.8",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "react-static-webpack-plugin": "^2.1.0",
    "recompose": "^0.22.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.14.0",
    "ts-loader": "^2.0.1",
    "typescript": "^2.2.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1"
  }
}
